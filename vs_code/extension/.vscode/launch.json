{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}/vs_code/extension"
			],
			"outFiles": [
				"${workspaceFolder}/vs_code/extension/out/**/*.js"
			],
			"preLaunchTask": "${defaultBuildTask}"
		}
	]
}
